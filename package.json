{
  "name": "common-constants",
  "version": "1.2.6",
  "description": "A set of constants commonly used across many projects, relating to e.g. date, time, ports, logging, and math",
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "compile-ts": "./node_modules/typescript/bin/tsc -p ./",
    "compile-ts:watch": "./node_modules/typescript/bin/tsc -p ./ --watch",
    "test": "mocha test/",
    "boilerplate-check": "./script/ensure-fully-implemented.rb",
    "test:watch": "chokidar ./lib/** ./config/** ./test/** --initial -c \"mocha test/\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andfaulkner/node-common-constants.git"
  },
  "keywords": [
    "constants",
    "date",
    "time",
    "logging",
    "log",
    "logs",
    "ports",
    "port",
    "defaults",
    "sane-defaults",
    "math",
    "maths",
    "mathematics",
    "science"
  ],
  "author": "Andrew Faulkner <andfaulkner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andfaulkner/node-common-constants/issues"
  },
  "homepage": "https://github.com/andfaulkner/node-common-constants#readme",
  "dependencies": { },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "chai": "^3.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "test-console": "^1.0.0",
    "typescript": "2.1.4"
  }
}
